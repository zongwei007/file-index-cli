{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Deno (Allow All)",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--config",
        "tsconfig.json",
        "--import-map",
        "import_map.json",
        "--inspect-brk",
        "-A",
        "mod.ts",
        "folder",
        "index",
        "E:\\TEMP",
        "--dry-run",
        "--verbose"
      ]
    }
  ]
}
